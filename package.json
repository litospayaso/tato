  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "release": "npm run build-prod && npm run replace-base-href && npm run deploy-ghpages && npm run display-release",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "ionic build --prod -- --base-href=\"https://litospayaso.github.io/tato/\"",
    "ionic-build": "ionic build --prod",
    "replace-base-href": "node ./scripts/replace.js",
    "deploy-ghpages": "npx angular-cli-ghpages --dir=www",
    "apk": "npm run ionic-build && ionic capacitor copy android && cd android && .\\gradlew assembleDebug && cd ..",
    "sign": ".sign\\sign.bat",
    "apk-assemble": "npm run ionic-build && ionic capacitor copy android && cd android && .\\gradlew bundleRelease && cd .. && npm run sign",
    "generate-assets": "capacitor-assets generate",
    "display-release": "node ./scripts/console.js"
  },
  "private": true,
